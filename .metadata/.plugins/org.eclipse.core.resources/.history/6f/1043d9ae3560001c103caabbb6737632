
void ProjectInit();
void ProjectRun();
void ProjectCleanup();

void SetTime(PmodRTCC *rtcc, RTCC_Time *time);
void GetTime(PmodRTCC *rtcc, RTCC_Time *time, RTCC_Target src);

typedef struct RTCC_Time {
   u8 second;
   u8 minute;
   u8 hour;
   u8 ampm;
   u8 day;
   u8 date;
   u8 month;
   u8 year;
} RTCC_Time;
